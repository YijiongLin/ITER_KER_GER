    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },

        {
            "name": "Python: FetchPick",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "pythonPath": "${config:python.pythonPath}",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "env": {},
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "--alg=her",
                "--env=FetchPickAndPlace-v1",
                "--num_timesteps=5000"

            ]
        },

        {
            "name": "Python: FetchPush",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "pythonPath": "${config:python.pythonPath}",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "env": {},
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "--alg=her",
                "--env=FetchPush-v1",
                "--num_timesteps=5000"

            ]
        },

        {
            "name": "Python: BaxterPush",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "pythonPath": "${config:python.pythonPath}",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "env": {},
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "--alg=her",
                "--env=BaxterPush-v1",
                "--num_timesteps=5000",
                "--n_rsym=2",
                "--n_PER=2"

            ]
        },

